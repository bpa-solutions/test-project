{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "list-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listWebPart/ListWebPartWebPart.js",
          "manifest": "./src/webparts/listWebPart/ListWebPartWebPart.manifest.json"
        }
      ]
    },
    "details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/details/DetailsWebPart.js",
          "manifest": "./src/webparts/details/DetailsWebPart.manifest.json"
        }
      ]
    },
    "helper-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/helper/HelperApplicationCustomizer.js",
          "manifest": "./src/extensions/helper/HelperApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ListWebPartWebPartStrings": "lib/webparts/listWebPart/loc/{locale}.js",
    "DetailsWebPartStrings": "lib/webparts/details/loc/{locale}.js",
    "HelperApplicationCustomizerStrings": "lib/extensions/helper/loc/{locale}.js"
  }
}
